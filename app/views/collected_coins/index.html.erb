<p id="notice"><%= notice %></p>

<h1>Collected Coins</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collected_coins.each do |collected_coin| %>
      <tr>
        <td><%= collected_coin.value %></td>
        <td><%= collected_coin.user %></td>
        <td><%= link_to 'Show', collected_coin %></td>
        <td><%= link_to 'Edit', edit_collected_coin_path(collected_coin) %></td>
        <td><%= link_to 'Destroy', collected_coin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collected Coin', new_collected_coin_path %>
